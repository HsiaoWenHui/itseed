		<!-- q = 有幾個"關於資種"的faq  -->
		<% var q = 8; %>
			<% for(var i = 0; i < q; i++){ %>
				<% if (datas[i] != null) { %>
					<div class="col-md-6 col-md-offset-3" id="<%= datas[i].id %>">
						<div class="faq">
							<a class="faq-heading faq-normal" data-toggle="collapse" data-parent="#accordion" href="#collapse<%= datas[i].id %>" class="collapsed">
								<%= datas[i].title %>
								<span class="triangle triangle-rotate"><img src="/images/layout/triangle.png"></span>
							</a>
							<div id="collapse<%= datas[i].id %>" class="faq-collapse collapse">
								<div class="faq-body">
									<%- datas[i].content %>
								</div>
							</div>
						</div>
					</div>
				<% } %>
			<% } %>
		</div>

