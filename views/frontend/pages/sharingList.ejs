<div class="six-hg-title">
  <div class="six-hg-tit leader-color">
      <span>經驗分享</span>
  </div>
</div>

<div class="container">
	<div class="row share-page">
		<div class="col-xs-12 share-but-part">
			<button class="share-but school-btn" onclick="school()">各校系怎麼看</button>
			<button class="share-but leader-btn" onclick="leader()">領導者怎麼看</button>
			<button class="share-but miwan-btn" onclick="miwan()">迷惘人怎麼看</button>
			<!-- <button class="share-but graduate-btn" onclick="graduate()">學長姐怎麼看</button> -->
		</div>
		<div class="col-xs-12 share-part">
			<% for(var i = 0; i < datas.length; i++){ %>
				<% if(datas[i].th == 1) { %>
					<div class="col-md-4 col-sm-6 school-block">
						<a href="<%= url + datas[i].id %>">
							<div class="school share-B" data-toggle="modal" data-target="#myModal">
								<img src="<%= datas[i].photo %>"></img>
								<div class="share-title">
									<h4><%= datas[i].name %></h4>
									<h5><%= datas[i].title %></h5>
								</div>
							</div>
							<span class="share-tag">經驗分享</span>
						</a>
					</div>
				<% } %>
				<% if(datas[i].th == 2) { %>
					<div class="col-md-4 col-sm-6 leader-block">
						<a href="<%= url + datas[i].id %>">
							<div class="leader share-B" data-toggle="modal" data-target="#myModal">
								<img src="<%= datas[i].photo %>"></img>
								<div class="share-title">
									<h4><%= datas[i].name %></h4>
									<h5><%= datas[i].title %></h5>
								</div>
							</div>
							<span class="share-tag">經驗分享</span>
						</a>
					</div>
				<% } %>
				<% if(datas[i].th == 3) { %>
					<div class="col-md-4 col-sm-6 miwan-block">
						<a href="<%= url + datas[i].id %>">
							<div class="miwan share-B" data-toggle="modal" data-target="#myModal">
								<img src="<%= datas[i].photo %>"></img>
								<div class="share-title">
									<h4><%= datas[i].name %></h4>
									<h5><%= datas[i].title %></h5>
								</div>
							</div>
							<span class="share-tag">經驗分享</span>
						</a>
					</div>
				<% } %>
				<% if(datas[i].th == 4) { %>
					<div class="col-md-4 col-sm-6 graduate-block">
						<a href="<%= url + datas[i].id %>">
							<div class="graduate share-B" data-toggle="modal" data-target="#myModal">
								<img src="<%= datas[i].photo %>"></img>
								<div class="share-title">
									<h4><%= datas[i].name %></h4>
									<h5><%= datas[i].title %></h5>
								</div>
							</div>
							<span class="share-tag">經驗分享</span>
						</a>
					</div>
				<% } %>
			<% } %>
