	</div>
</div>

		<script>

			// $(".share-tag").mouseover(function(){
			// 	$(".share-B").css("opacity","0.7");
			// });
			// $(".share-tag").mouseout(function(){
			// 	$(".share-B").css("opacity","1");
			// });

           function school(){
           		$('.school-block').css('display','block');
           		$('.school-btn').css('background-color','rgb(255,195,19)');
           		$('.school-btn').css('color','white');
           		$('.leader-block').css('display','none');
           		$('.leader-btn').css('background-color','white');
           		$('.leader-btn').css('color','black');
           		$('.miwan-block').css('display','none');
           		$('.miwan-btn').css('background-color','white');
           		$('.miwan-btn').css('color','black');
           		$('.graduate-block').css('display','none');
           		$('.graduate-btn').css('background-color','white');
           		$('.graduate-btn').css('color','black');
           }
           function leader(){
           		$('.school-block').css('display','none');
           		$('.school-btn').css('background-color','white');
           		$('.school-btn').css('color','black');
           		$('.leader-block').css('display','block');
           		$('.leader-btn').css('background-color','rgb(83,194,196)');
           		$('.leader-btn').css('color','white');
           		$('.miwan-block').css('display','none');
           		$('.miwan-btn').css('background-color','white');
           		$('.miwan-btn').css('color','black');
           		$('.graduate-block').css('display','none');
           		$('.graduate-btn').css('background-color','white');
           		$('.graduate-btn').css('color','black');
           }
           function miwan(){
           		$('.school-block').css('display','none');
           		$('.school-btn').css('background-color','white');
           		$('.school-btn').css('color','black');
           		$('.leader-block').css('display','none');
           		$('.leader-btn').css('background-color','white');
           		$('.leader-btn').css('color','black');
           		$('.miwan-block').css('display','block');
           		$('.miwan-btn').css('background-color','rgb(84,199,138)');
           		$('.miwan-btn').css('color','white');
           		$('.graduate-block').css('display','none');
           		$('.graduate-btn').css('background-color','white');
           		$('.graduate-btn').css('color','black');
           }
           function graduate(){
           		$('.school-block').css('display','none');
           		$('.school-btn').css('background-color','white');
           		$('.school-btn').css('color','black');
           		$('.leader-block').css('display','none');
           		$('.leader-btn').css('background-color','white');
           		$('.leader-btn').css('color','black');
           		$('.miwan-block').css('display','none');
           		$('.miwan-btn').css('background-color','white');
           		$('.miwan-btn').css('color','black');
           		$('.graduate-block').css('display','block');
           		$('.graduate-btn').css('background-color','rgb(81,158,247)');
           		$('.graduate-btn').css('color','white');
           }
           school();
        </script>

